<!DOCTYPE html>
<html class="fixed">
<head>
    <!-- Vendor CSS -->
    <!--====== Required meta tags ======-->
    <link rel="shortcut icon" href="Theme/images/favicon.png" type="image/png">
    <!--====== Title ======-->
    <title>@ViewBag.Title  | Upcycle4Good</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- css -->
    @*<link rel="stylesheet" href="@Url.Content("~/Skydash/vendors/feather/feather.css")">
        <link rel="stylesheet" href="@Url.Content("~/Skydash/vendors/ti-icons/css/themify-icons.css")">
        <link rel="stylesheet" href="@Url.Content("~/Skydash/vendors/css/vendor.bundle.base.css")">*@
    <!-- endinject -->
    <!-- Plugin css for this page -->
    @*<link rel="stylesheet" href="@Url.Content("~/Skydash/vendors/ti-icons/css/themify-icons.css")">*@
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    @*<link rel="stylesheet" href="@Url.Content("~/Skydash/css/vertical-layout-light/style.css")">*@
    <!-- endinject -->
    @*<link rel="shortcut icon" href="~/Skydash/images/favicon.png" />*@

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style type="text/css">
        .main-panel {
            width: 100% !important;
        }
    </style>
</head>
<body>
  

    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        @Html.Partial("_header")

        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_settings-panel.html -->
            <div class="theme-setting-wrapper">
                <div id="settings-trigger"><i class="ti-settings"></i></div>
                <div id="theme-settings" class="settings-panel">
                    <i class="settings-close ti-close"></i>
                    <p class="settings-heading">SIDEBAR SKINS</p>
                    <div class="sidebar-bg-options selected" id="sidebar-light-theme"><div class="img-ss rounded-circle bg-light border mr-3"></div>Light</div>
                    <div class="sidebar-bg-options" id="sidebar-dark-theme"><div class="img-ss rounded-circle bg-dark border mr-3"></div>Dark</div>
                    <p class="settings-heading mt-2">HEADER SKINS</p>
                    <div class="color-tiles mx-0 px-4">
                        <div class="tiles success"></div>
                        <div class="tiles warning"></div>
                        <div class="tiles danger"></div>
                        <div class="tiles info"></div>
                        <div class="tiles dark"></div>
                        <div class="tiles default"></div>
                    </div>
                </div>
            </div>

            @*@Html.Partial("_rightSidebar")*@
            <!-- partial -->
            <!-- partial:partials/_sidebar.html -->
            @*@Html.Partial("_leftSidebar")*@
            <!-- partial -->

            <div class="main-panel">
                @RenderBody()
               
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                @Html.Partial("_copyRightFooter")
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="@Url.Content("~/Skydash/vendors/js/vendor.bundle.base.js")"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    @*<script src="vendors/chart.js/Chart.min.js"></script>
        <script src="vendors/datatables.net/jquery.dataTables.js"></script>
        <script src="vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
        <script src="js/dataTables.select.min.js"></script>*@
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="@Url.Content("~/Skydash/js/off-canvas.js")"></script>
    <script src="@Url.Content("~/Skydash/js/hoverable-collapse.js")"></script>
    <script src="@Url.Content("~/Skydash/js/template.js")"></script>
    <script src="@Url.Content("~/Skydash/js/settings.js")"></script>
    <script src="@Url.Content("~/Skydash/js/todolist.js")"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="@Url.Content("~/Skydash/js/dashboard.js")"></script>
    <script src="@Url.Content("~/Skydash/js/Chart.roundedBarCharts.js")"></script>

    @RenderSection("scripts", required: false)

</body>
</html>
