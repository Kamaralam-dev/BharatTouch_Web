
@using BharatTouch.CommonHelper
@{
    Layout = null;
    var code = ViewBag.Code != null ? ViewBag.Code : "";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/SmartTheme/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/SmartTheme/css/slick-theme.css">
    <link rel="stylesheet" href="~/SmartTheme/css/slick.css">
    <link rel="stylesheet" href="~/SmartTheme/font/fonts.css">
    <link rel="stylesheet" href="~/SmartTheme/css/style.css" />
    <link rel="stylesheet" href="~/SmartTheme/css/media.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/javascripts/custom/pnotify/pnotify.css" />
    <title>Bharat Touch</title>
    <link rel="icon" href="/SmartTheme/images/bharattouchfavico.ico" style="border-radius: 30px;" type="image/x-icon">

    <style>
        .modal-body ul {
            color: #353131;
        }

        .modal-body h5 {
            color: #FF6821 !important;
        }
    </style>


    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-3VWMPTNPEL"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-3VWMPTNPEL');
    </script>
    <meta name="google-site-verification" content="Bxtl3yy4-p-fMpDJiZ2yWQWuk3DgF-TFqQkLDb5m8mo" />

</head>
<body>


    <div class="modal fade" id="termsOfServicesModal" tabindex="-1" aria-labelledby="termsOfServicesModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-scrollable">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title text-bold" id="termsOfServicesModalLabel">Bharat Touch - Terms Of Services</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body px-5">
                    @Html.Partial("_termsAndServices")
                </div>
                <div class="modal-footer">
                    <div class="my-2 d-flex justify-content-between align-items-center">
                        <button type="button" class="btn btn-secondary align-self-end" data-bs-dismiss="modal" aria-label="Close">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="privacyPolicyModal" tabindex="-1" aria-labelledby="privacyPolicyModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg modal-dialog-scrollable">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title text-bold" id="privacyPolicyModalLabel">Bharat Touch - Privacy Policy</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body px-5">
                    @Html.Partial("_privacyPolicy")
                </div>
                <div class="modal-footer">
                    <div class="my-2 d-flex justify-content-between align-items-center">
                        <button type="button" class="btn btn-secondary align-self-end" data-bs-dismiss="modal" aria-label="Close">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <header>
        <nav class="navbar navbar-expand-lg">
            <div class="container">
                <a class="navbar-brand" href="/"><img src="~/SmartTheme/images/logo.svg" alt="logo"></a>
            </div>
        </nav>
    </header>

    <section class="singup-form-outer">

        <input type="hidden" id="hdnReferalCode" value="@code" />
        @Html.Hidden("hdnUserId", 0)
        @Html.Hidden("hdnOrderId", 0)

        <div class="container">
            <div class="get-started-text">
                <h1 class="title-44">Get Started with Bharat Touch</h1>
                <p>Create your account, customize your card, and place your order in just a few easy steps. Secure, personalized, and designed to grow your digital identity.</p>
            </div>

            <div class="step-form-outer">
                <div class="step-points">
                    <div class="step-points-inner">
                        <div class="step-point" data-step="sign-up">
                            <span class="step-image">
                                <img src="~/SmartTheme/images/user.svg">
                            </span>
                            <i>Sign Up</i>
                            <span class="progress-line">
                        </div>
                        <div class="step-point" data-step="card-options">
                            <span class="step-image">
                                <img src="~/SmartTheme/images/card-options.svg">
                            </span>
                            <i>Card Options</i>
                            <span class="progress-line">
                        </div>
                        <div class="step-point" data-step="business-info">
                            <span class="step-image">
                                <img src="~/SmartTheme/images/business-info.svg">
                            </span>
                            <i>Business Info</i>
                            <span class="progress-line">
                        </div>
                        <div class="step-point" data-step="shipping-billing">
                            <span class="step-image">
                                <img src="~/SmartTheme/images/shipping.svg">
                            </span>
                            <i>Shipping & Billing</i>
                            <span class="progress-line">
                        </div>
                        <div class="step-point" data-step="finish">
                            <span class="step-image">
                                <img src="~/SmartTheme/images/finish.svg">
                            </span>
                            <i>Complete Order</i>
                            <span class="progress-line">
                        </div>
                    </div>

                    <div class="sign-up-form">
                        <div id="sign-up" class="form-step active-step">
                            <h2>Sign Up</h2>
                            @using (Html.BeginForm("Create", "Users", FormMethod.Post, new { id = "CreateUserForm" }))
                            {
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>First Name</label>
                                        @Html.TextBox("FirstName", "")
                                    </div>
                                    <div class="col-md-6">
                                        <label>Last Name</label>
                                        @Html.TextBox("LastName", "")
                                    </div>
                                    <div class="col-md-6">
                                        <label>Display Name</label>
                                        @Html.TextBox("Displayname", "")
                                    </div>
                                    <div class="col-md-6">
                                        <label>Email</label>
                                        @Html.TextBox("EmailId", "")
                                    </div>
                                    <div class="col-md-6">
                                        <label>Country Code</label>
                                        @Html.Hidden("WhatsAppCountryId", 0)
                                        @Html.Hidden("CountryId")
                                        @Html.DropDownList("ddlCountryId", (IEnumerable<SelectListItem>)ViewBag.Countries, "Select Country Code", new { @id = "ddlCountryId" })
                                    </div>
                                    <div class="col-md-6">
                                        <label>Phone</label>
                                        @Html.TextBox("Phone", "", new { @class = "digitonly" })
                                    </div>
                                    <div class="col-md-6">
                                        <label>Password</label>
                                        @Html.TextBox("Password", "", new { @type = "password", @id = "txtRegPassword" })
                                    </div>
                                    <div class="col-md-6">
                                        <label>Confirm Password</label>
                                        @Html.TextBox("ConfirmPassword", "", new { @type = "password", @id = "txtRegConfirmPassword" })
                                    </div>
                                    @*<div class="col-md-6">
                            <label>Select Pack</label>
                            <select>
                                <option>Family Pack (11499.00)</option>
                                <option>Family Pack (12499.00)</option>
                            </select>
                        </div>*@
                                    <div class="col-md-6">
                                        <label>Enter Referral Code</label>
                                        @Html.TextBox("ReferredByCode", "")
                                        <div class="error" id="invalidReferalCode"></div>
                                    </div>
                                    <div class="col-md-6">
                                        <div id="grecaptchaSignup"></div>
                                        <input type="hidden" name="hdngrecaptchaResponseSignup" id="hdngrecaptchaResponseSignup" value="" />
                                    </div>
                                    <div class="col-md-12 privacy-checkbox">
                                        <input type="checkbox" id="chkPrivacyPolicy">
                                        <label for="chkPrivacyPolicy"><span class="checkbox-mark"></span><div>I have read and agree the <a class="underline" id="termsOfServices_link" style="cursor: pointer; color: #ff8c56;">Terms of Services</a> and the <a class="underline" id="privacyPolicy_link" style="cursor: pointer; color: #ff8c56;">Privacy Policy.</a></div></label>
                                    </div>


                                    <div class="col-md-12 text-center">
                                        <button type="submit" class="btn-gradient next"> <div class="lds-dual-ring"></div>&nbsp;Sign Up</button>
                                    </div>
                                    <div class="already-member">
                                        <p> <a href="/home/login" style="text-decoration: underline;">Already a member ? Log In</a></p>
                                    </div>
                                </div>
                            }
                        </div>

                        <!-- Choose Your Card Style -->

                        <div id="card-options" class="form-step ">
                            <h2>Choose Your Card Style</h2>
                            <form>
                                <div class="row">
                                    <div class="col-md-6 card-boxes">
                                        <h3>1. Choose Color </h3>
                                        <div class="select-cards">
                                            <div class="select-cards-inner">
                                                @*<input type="radio" name="choose-color" checked value="2">*@
                                                <input type="radio" name="CardColorId" data-color="Gold" data-bg-color="#e8d087" data-txt-color="#000000" checked value="2">
                                                <div class="checkbox-card">
                                                    <p>
                                                        <img src="~/SmartTheme/images/gold-card.png">
                                                    </p>
                                                    <span>Gold</span>
                                                </div>
                                            </div>
                                            <div class="select-cards-inner">
                                                <input type="radio" name="CardColorId" data-color="Silver" data-bg-color="#e4e4e2" data-txt-color="#000000" value="3">
                                                <div class="checkbox-card">
                                                    <p>
                                                        <img src="~/SmartTheme/images/white-card.png">
                                                    </p>
                                                    <span>Silver</span>
                                                </div>
                                            </div>
                                            <div class="select-cards-inner">
                                                <input type="radio" name="CardColorId" data-color="Black" data-bg-color="#000000" data-txt-color="#ffffff" value="1">
                                                <div class="checkbox-card">
                                                    <p>
                                                        <img src="~/SmartTheme/images/black-card.png">
                                                    </p>
                                                    <span>Black</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6 card-boxes">
                                        <h3>2. Pick Your Card Finish</h3>
                                        <div class="select-cards" style=" display: block; line-height: 2;">
                                            <div class="select-cards-inner">
                                                <input type="radio" name="CardFinishId" data-finish-name="Glossy Finish"  value="2">
                                                <div class="checkbox-card">
                                                    @*<p>
                                            <img src="~/SmartTheme/images/gold-card.png">
                                        </p>*@
                                                    <span>Glossy Finish</span>
                                                </div>
                                            </div>
                                            <div class="select-cards-inner">
                                                @*<input type="radio" name="pick-card" checked>*@
                                                <input type="radio" name="CardFinishId" data-finish-name="Matte Finish" checked value="1">
                                                <div class="checkbox-card">
                                                    <span>Matte Finish</span>
                                                </div>
                                            </div>
                                            <div class="select-cards-inner">
                                                <input type="radio" name="CardFinishId" data-finish-name="Clear Finish" value="3">
                                                <div class="checkbox-card">
                                                    @*<p>
                                                        <img src="~/SmartTheme/images/gold-card.png">
                                                    </p>*@
                                                    <span>Clear Finish</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card-material">
                                            @*<h3>3. Add a premium metal NFC card along with the plastic card (+₹ 3,499.00).</h3>*@
                                            <!--<div class="card-material-checkbox">-->
                                                @*<input type="checkbox" name="card-material" id="metal-card">*@
                                                <!--<input type="checkbox" name="card-material" id="IncludeMetalCard">
                                                <label for="IncludeMetalCard" style=" width: max-content;"><span class="card-checkmark"></span>Yes, include a metal NFC card.</label>
                                            </div>-->
                                            <h3>3. Pick Your Card Type</h3>
                                            <div>
                                                <div class="select-cards-inner" style="margin-bottom: 8px;">
                                                    <input type="radio" name="CardTypeId" data-type="PVC Card" data-price="1999" checked value="1">
                                                    <div class="checkbox-card">
                                                        <span>PVC Card - Rs.1999</span>
                                                    </div>
                                                </div>
                                                <div class="select-cards-inner" style="margin-bottom: 8px;">
                                                    <input type="radio" name="CardTypeId" data-type="Metal Card" data-price="3499" value="2">
                                                    <div class="checkbox-card">
                                                        <span>Metal Card - Rs.3499</span>
                                                    </div>
                                                </div>

                                                <div class="select-cards-inner" style="margin-bottom: 8px;">
                                                    <input type="radio" name="CardTypeId" data-type="Combo Card" data-price="4499" value="3">
                                                    <div class="checkbox-card">
                                                        <span style="color: #9ea2a2; margin-bottom: 1px;">Combo (Metal + PVC) - <span class="no-before-radio" style="padding-left: 0; text-decoration: line-through;">Rs.5499</span> Rs.4499</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                            </form>
                        </div>
                        <div class="bottom-btns">
                            <a href="#" class="btn-grey back">Back</a>
                            <a id="btnNextSelectNfc" href="javascript:void(0)" class="btn-gradient next">Next</a>
                        </div>
                    </div>

                    <!-- Tell Us About Your Business -->
                    <div id="business-info" class="form-step">
                        <h2>Tell Us About Your Business</h2>
                        @using (Html.BeginForm("UpdateUserNfcDetails", "Users", FormMethod.Post, new { id = "nfcCardDetailForm" }))
                        {
                            <div class="row">
                                <div class="col-md-7 choosen-card-outer">
                                    <p>Choosen Card Details:</p>
                                    <div class="choosen-card">
                                        <div class=""> @*its class = choosen-card-img*@
                                            @*<img src="~/SmartTheme/images/gold-card.png" alt="gold-card">*@
                                            @Html.Partial("_nfcCardPreview", new DataAccess.ViewModels.NfcCardColorViewModel())
                                        </div>
                                        <div class="choosen-card-info">
                                            <p id="choosenCardColor"><span>Card Color</span>Gold</p>
                                            <p id="choosenCardFinish"><span>Card Finish</span>Glossy</p>
                                            <p id="choosenCardMaterial"><span>Card Material</span>NFC Metal Card</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-5 choose-card-detail">
                                    <div class="mb-2">
                                        <label>Your Name / Business Name</label>
                                        <input type="text" id="NfcCardLine1" required placeholder="Your Name / Business Name">
                                    </div>
                                    <div class="mb-2">
                                        <label>Designation / Business Name</label>
                                        <input type="text" id="NfcCardLine2" placeholder="Designation / Business Name">
                                    </div>
                                    <div class="mb-2">
                                        <label>Business Name / Phone Number</label>
                                        <input type="text" id="NfcCardLine3" placeholder="Business Name / Phone Number">
                                    </div>
                                </div>
                            </div>
                            <div class="bottom-btns">
                                <a href="#" class="btn-grey back">Back</a>
                                <button type="submit" class="btn-gradient next">Next</button>
                            </div>
                        }
                    </div>


                    <!-- Billing & Shipping Details -->

                    @using (Html.BeginForm("UserAllWizardDetails", "Users", FormMethod.Post, new { id = "orderUpsertForm", @class = "php-email-form" }))
                    {
                        <div id="shipping-billing" class="form-step">
                            <h2>Enter Your Billing & Shipping Details</h2>
                            <div class="row shipping-details">
                                <div class="col-md-6">
                                    <div class="row">
                                        <div class="col-6 col-md-6">
                                            <h4>SHIPPING ADDRESS</h4>
                                        </div>
                                        <div class="col-6 col-md-6 shipping-checkbox" style="display:none;">
                                            @*<input type="checkbox" id="shipping-address">*@
                                            <input type="checkbox" id="IsSelfPick">
                                            <label for="IsSelfPick"><span class="checkbox-mark"></span> Self Pick</label>
                                        </div>
                                        <div class="col-md-12">
                                            <label>Address Line 1</label>
                                            <input type="text" id="ShippingAddress1" name="ShippingAddress1" placeholder="Address 1">
                                        </div>
                                        <div class="col-md-12">
                                            <label>Address Line 2</label>
                                            <input type="text" id="ShippingAddress2" name="ShippingAddress2" placeholder="Address 2">
                                        </div>
                                        <div class="col-md-6">
                                            <label>City</label>
                                            <input type="text" id="ShippingCity" name="ShippingCity" placeholder="City">
                                        </div>
                                        <div class="col-md-6">
                                            <label>State</label>
                                            <input type="text" id="ShippingState" name="ShippingState" placeholder="State">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Country</label>
                                            <input type="text" id="ShippingCountry" name="ShippingCountry" placeholder="Country">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Enter Zip Code</label>
                                            <input type="text" id="ShippingZip" name="ShippingZip" placeholder="ZIP">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <div class="col-6 col-md-6">
                                            <h4>BILLING ADDRESS</h4>
                                        </div>
                                        <div class="col-6 col-md-6 shipping-checkbox">
                                            @*<input type="checkbox" id="shipping-address">*@
                                            <input type="checkbox" id="chkSameShippingDetails">
                                            <label for="chkSameShippingDetails"><span class="checkbox-mark"></span> Same as shipping address</label>
                                        </div>
                                        <div class="col-md-12">
                                            <label>Address Line 1</label>
                                            <input type="text" id="BillingAddress1" name="BillingAddress1" placeholder="Address 1">
                                        </div>
                                        <div class="col-md-12">
                                            <label>Address Line 2</label>
                                            <input type="text" id="BillingAddress2" name="BillingAddress2" placeholder="Address 2">
                                        </div>
                                        <div class="col-md-6">
                                            <label>City</label>
                                            <input type="text" id="BillingCity" name="BillingCity" placeholder="City">
                                        </div>
                                        <div class="col-md-6">
                                            <label>State</label>
                                            <input type="text" id="BillingState" name="BillingState" placeholder="State">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Country</label>
                                            <input type="text" id="BillingCountry" name="BillingCountry" placeholder="Country">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Enter Zip Code</label>
                                            <input type="text" id="BillingZip" name="BillingZip" placeholder="ZIP">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="bottom-btns">
                                <a href="#" class="btn-grey back">Back</a>
                                <button id="btnShippingDetailSubmitValidate" type="submit" class="btn-gradient next"> <div class="lds-dual-ring"></div>&nbsp;Next</button>
                            </div>
                        </div>
                    }


                    <!-- Review Your Order -->
                    @using (Html.BeginForm("VerifyAndSaveNfcOrderPayment", "Users", FormMethod.Post, new { id = "orderPaymentForm" }))
                    {
                        @Html.Hidden("PaymentMethodId", 2)
                        @Html.Hidden("PaymentAmount", 0)
                        @Html.Hidden("hdnInitialDiscountedPackageCode", 0)
                        @Html.Hidden("DiscountedPackageCost", 0)
                        @Html.Hidden("hdnInitialTax", 0)
                        @Html.Hidden("Tax", 0)
                        @Html.Hidden("ShippingAmount", 0)
                        @Html.Hidden("CouponDiscount", 0)
                        @Html.Hidden("WantMetalCard", false)
                <div id="finish" class="form-step">
                    <h2>Review Your Order & Complete Payment</h2>

                    @*28-May*@
                    <div class="order-complete-payments">
                        <div class="order-complete-row">
                            <h3><img src="~/SmartTheme/images/card-type-icon.svg" alt="Image">Card Selected</h3>
                            <div class="order-complete-row-card-type">
                                <div class="grid1">
                                    @*<img src="~/SmartTheme/images/card-tpe.png">*@
                                    @Html.Partial("_nfcCardPreview", new DataAccess.ViewModels.NfcCardColorViewModel())
                                </div>
                                <div class="grid2">
                                    <table style="width: 100%; border-collapse: collapse; color: white;">
                                        <tr>
                                            <td><span>Card Color</span> <strong id="choosenCardColorFinalStep">Gold</strong> </td>
                                            <td><span>Card Finish</span> <strong id="choosenCardFinishFinalStep">Glossy</strong> </td>
                                            <td><span>Card Material</span> <strong id="choosenCardMaterialFinalStep">NFC Metal Card</strong> </td>
                                        </tr>
                                        <tr>
                                            @*<td><span>Your Name / Business Name</span> <strong class="textNfcCardLine1"></strong> </td>
                                            <td><span>Designation / Business Name</span> <strong class="textNfcCardLine2"></strong> </td>
                                            <td><span>Business Name / Phone Number</span> <strong class="textNfcCardLine3"></strong> </td>*@
                                            @*<td><span>Delivery Method</span> <strong>Pick by self</strong> </td>*@
                                            @*<tr>
                                                <th>Delivery Method</th>
                                                <td>
                                                    <div class="delivery-method"><input type="radio" name="delivery-method" id="pick-by-self" checked><label for="pick-by-self">Pick by self</label></div>
                                                    <div class="delivery-method"><input type="radio" name="delivery-method" id="deliver-to-address"><label for="deliver-to-address">Deliver to address</label></div>
                                                </td>
                                            </tr>*@
                                        </tr>
                                    </table>

                                </div>
                                @*<div class="grid3">
                                    <h6 id="mainPackageCost">Rs. 0</h6>
                                </div>*@
                            </div>
                        </div>

                        <div class="order-complete-row">
                            <h3><img src="~/SmartTheme/images/discount-tag.svg" alt="Image">Price / Discount</h3>
                            <div class="order-complete-row-card-type discount-row">
                                <table style="width: 100%; border-collapse: collapse; color: white;">

                                    <tr>
                                        <td>Price</td>
                                        <td><strong id="mainPackageCost">Rs. 0</strong> </td>
                                    </tr>
                                    <tr id="metalCardPriceContainer" style="display: none;">
                                        <td>Metal Card</td>
                                        <td><strong id="displayMetalCardPrice">Rs. 3499</strong></td>
                                    </tr>
                                    <tr>
                                        <td>Referral Discounted</td>
                                        <td><strong id="displayReferralDiscount">Rs. 0</strong> </td>
                                    </tr>
                                    <tr>
                                        <td>Coupon Code</td>
                                        <td>
                                            <div class="enter-discount-couponn">
                                                @*<a id="btnCancelDiscountCoupon" href="javascript:void(0)" style=" color: red; margin-left: 17px;">Cancel</a>*@

                                                <img src="~/SmartTheme/images/coupon-code-icon.svg">
                                                <input type="text" id="DiscountCoupon" name="DiscountCoupon" placeholder="Enter Discount Coupon" class="discount-feild">
                                                <input type="button" value="APPLY" id="btnApplyDiscountCoupon" href="javascript:void(0)" class="apply-btn-feild">
                                                <input type="button" value="CANCEL" style="display:none;" id="btnCancelDiscountCoupon" href="javascript:void(0)" class="cancel-coupon-btn-feild">
                                            </div>
                                        </td>
                                    </tr>
                                    <tr id="CouponDiscountContainer" style="display: none;">
                                        <td>Discount Amount</td>
                                        <td><strong id="displayCouponDiscount">Rs. 0</strong> </td>
                                    </tr>
                                </table>
                            </div>
                        </div>

                        <div class="order-complete-row payable-amount">
